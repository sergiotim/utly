generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Link {
  id             String   @id @default(cuid())
  originalUrl    String   @db.Text
  slug           String   @unique
  createdAt      DateTime @default(now())
  lastAccessedAt DateTime @default(now())

  @@index([slug])
  @@index([lastAccessedAt])
}
